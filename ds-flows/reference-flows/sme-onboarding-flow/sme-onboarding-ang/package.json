{
  "name": "sme-onboarding-ang",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "setup": "mvn dependency:copy -Dartifact=com.backbase.web-sdk.collection:collection-bb-web-sdk:1.16.4:zip -DoutputDirectory=collections",
    "build": "bb-ang build-apps --output-hashing --build-memory=4096 --serial-build",
    "pre:start": "sdlc convert-from apps/employee/sdlc/experience.json apps/employee/sdlc/pages/home.json --output apps/employee/src/environments/simplified-models/home.json --remove-defaults false && sdlc convert-from apps/prospect/sdlc/experience.json apps/prospect/sdlc/pages/home.json --output apps/prospect/src/environments/simplified-models/home.json --remove-defaults false && sdlc convert-from apps/application-center/sdlc/experience.json apps/application-center/sdlc/pages/home.json --output apps/application-center/src/environments/simplified-models/home.json --remove-defaults false",
    "start:employee-app": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve employee --port 4200",
    "start:prospect-app": "ng serve prospect --port 4201",
    "start:application-center": "ng serve application-center --port 4202",
    "create:docker:container:employee": "sdlc package apps/employee/sdlc/experience.json --environment docker",
    "create:docker:container:prospect": "sdlc package apps/prospect/sdlc/experience.json --environment docker",
    "create:docker:container:application-center": "sdlc package apps/application-center/sdlc/experience.json --environment docker",
    "package:app:employee": "sdlc package apps/employee/sdlc/experience.json -e=bas",
    "package:app:employee-identity": "sdlc package apps/employee/sdlc/experience.json",
    "package:app:appcenter": "sdlc package apps/application-center/sdlc/experience.json",
    "package:app:prospect": "sdlc package apps/prospect/sdlc/experience.json",
    "package:apps": "npm run package:app:employee && npm run package:app:employee-identity && npm run package:app:prospect && npm run package:app:appcenter",
    "package:libs": "bb-ang package-libs",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.ts\"",
    "lint": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng lint",
    "test": "ng test --pass-with-no-tests --detect-open-handles --coverage",
    "test:watch": "ng test --project=cm-create-business-case-widget --watch",
    "update": "ng update @bb-cli/schematics@^4 @backbase/foundation-ang@^6"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/localize": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@backbase/backbase-theme": "~2.242.1",
    "@backbase/backbase-theme-retail-preset": "^1.0.26",
    "@backbase/case-management-ui-ang": "^3.1.1",
    "@backbase/cm-aml-results-widget": "^0.3.1",
    "@backbase/cm-business-relations-results-widget": "^0.1.2",
    "@backbase/cm-risk-assessment-results-widget": "^0.3.0",
    "@backbase/company-lookup-interaction-tasks": "^0.2.4",
    "@backbase/content-ang": "^4.0.0",
    "@backbase/djm-task-details-view-ui": "^2.0.2",
    "@backbase/ds-business-relations-interaction-task": "^0.2.0",
    "@backbase/ds-case-common-ang": "^1.0.0",
    "@backbase/ds-iam-common-ang": "^1.0.0",
    "@backbase/ds-shared-ang": "^2.3.1",
    "@backbase/flow-casedata-openapi-data": "^3.0.9",
    "@backbase/flow-interaction-openapi-data": "^2.0.17",
    "@backbase/flow-interaction-sdk-ang": "^3.0.0",
    "@backbase/foundation-ang": "6.13.1",
    "@backbase/lib-test-widget": "^2.3.0",
    "@backbase/ono-journey-collection-ang": "^0.5.0",
    "@backbase/sdlc-convert": "^0.13.16",
    "@backbase/ui-ang": "^6.31.1",
    "@backbase/universal-ang": "7.0.0",
    "@casl/ability": "^4.1.6",
    "@casl/angular": "^5.1.3",
    "@ng-bootstrap/ng-bootstrap": "~9.0.2",
    "@ng-select/ng-select": "^6.0.0",
    "@ngrx/component": "^11.1.1",
    "@ngrx/effects": "^11.1.1",
    "@ngrx/store": "^11.1.1",
    "@ngx-formly/core": "^5.5.10",
    "@swimlane/ngx-charts": "^16.0.0",
    "ajv": "^6.10.2",
    "bpmn-js": "^4.0.3",
    "core-js": "^2.6.11",
    "dmn-js": "^7.0.1",
