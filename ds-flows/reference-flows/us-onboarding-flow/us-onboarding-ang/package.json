{
  "name": "onboarding-us-ang-reference",
  "version": "1.6.7",
  "license": "MIT",
  "scripts": {
    "sdlc:pull-web-sdk": "mvn dependency:copy -Dartifact=com.backbase.web-sdk.collection:collection-bb-web-sdk:1.16.4:zip -DoutputDirectory=collections",
    "sdlc:build": "npm run package:libs && npm run package:apps",
    "sdlc:create-experience:onboarding": "bb model jm2cx ./apps/journey/sdlc/experience.json -e=bas",
    "sdlc:create-experience:case-manager": "bb model jm2cx ./apps/case-manager/sdlc/experience.json -e=bas",
    "sdlc:create-experience:case-manager-identity": "bb model jm2cx ./apps/case-manager/sdlc/experience.json",
    "sdlc:create-docker-container:onboarding": "bb docker jm2dc ./apps/journey/sdlc/experience.json --environment docker",
    "sdlc:create-docker-container:case-manager": "bb docker jm2dc ./apps/case-manager/sdlc/experience.json --environment docker",
    "sdlc:build-model:prospect": "bb model jm2sm apps/journey/sdlc/experience.json apps/journey/sdlc/pages/prospect.json -l silent --remove-defaults false --output apps/journey/sdlc/bundles/simplified-model-prospect.json",
    "sdlc:build-model:co-applicant": "bb model jm2sm apps/journey/sdlc/experience.json apps/journey/sdlc/pages/co-applicant.json -l silent --remove-defaults false --output apps/journey/sdlc/bundles/simplified-model-co-applicant.json",
    "sdlc:build-model:case-manager": "bb model jm2sm apps/case-manager/sdlc/experience.json apps/case-manager/sdlc/pages/home.json -l silent --remove-defaults false --output apps/case-manager/sdlc/bundles/simplified-model.json",
    "start:prospect": "bb-ang copy-assets && ng serve journey --port 4201 -c prospect",
    "start:prospect:ru": "bb-ang copy-assets && ng serve journey --port 4201 -c prospect-ru",
    "start:co-applicant": "bb-ang copy-assets && ng serve journey --port 4203 -c co-applicant",
    "start:case-manager": "bb-ang copy-assets && ng serve case-manager --port 4202",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.ts\"",
    "lint": "NODE_OPTIONS=--max-old-space-size=4096 node_modules/eslint/bin/eslint.js --config ./.eslintrc.json .",
    "test": "ng test",
    "package:apps": "bb-ang build-apps --output-hashing --build-memory=4096",
    "package:libs": "bb-ang package-libs",
    "ng": "ng",
    "i18n:extract": "ng extract-i18n --output-path locales"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "~11.2.10",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/localize": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@backbase/case-management-ui-ang": "3.1.1",
    "@backbase/cm-aml-results-widget": "0.3.1",
    "@backbase/ds-shared-ang": "2.3.1",
    "@backbase/flow-interaction-openapi-data": "^2.0.18",
    "@backbase/flow-interaction-sdk-ang": "^3.0.0",
    "@backbase/foundation-ang": "^6.13.2",
    "@backbase/ono-journey-collection-ang": "0.5.0",
    "@backbase/ui-ang": "^6.26.0",
    "@backbase/universal-ang": "^7.1.0",
    "@casl/ability": "^4.1.6",
    "@casl/angular": "^5.1.3",
    "@microsoft/api-extractor": "^7.18.19",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@ng-select/ng-select": "^7.2.0",
    "@ngrx/component": "^11.0.0",
    "@ngrx/effects": "^11.1.1",
    "@ngrx/store": "^11.1.1",
    "@ngx-formly/core": "^5.10.22",
    "@swimlane/ngx-charts": "^16.0.0",
    "ajv": "^6.10.2",
    "bpmn-js": "^4.0.0",
    "core-js": "^2.6.11",
    "dmn-js": "^10.3.0",
    "rxjs": "^6.6.6",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1102.13",
    "@angular-devkit/build-angular": "0.1102.13",
    "@angular-devkit/schematics": "^11.2.13",
    "@angular-eslint/builder": "4.3.0",
    "@angular-eslint/eslint-plugin": "4.3.0",
    "@angular-eslint/eslint-plugin-template": "4.3.0",
    "@angular-eslint/schematics": "4.3.0",
    "@angular-eslint/template-parser": "4.3.0",
    "@angular/cli": "^11.2.13",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@backbase/backbase-theme": "^2.284.1",
    "@backbase/backbase-theme-retail-preset": "^1.23.0",
    "@bb-cli/bb": "^3.1.3",
    "@bb-cli/bb-ang": "5.1.5",
    "@bb-cli/schematics": "^5.0.1",
    "@schematics/angular": "^10.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.11.8",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-no-null": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.5.0 ",
    "sass": "^1.34.1",
    "ts-node": "~7.0.0",
    "typescript": "~4.1.5"
  },
  "engines": {
    "node": "^>=10.9.0 <11.0.0",
    "npm": "6.0.1"
  },
  "backbase": {
    "defaultScope": "backbase"
  }
}
